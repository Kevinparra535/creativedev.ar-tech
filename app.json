          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-asset",
      [
        "expo-camera",
        {
          "cameraPermission": "Permite acceso a la c치mara para experiencias AR.",
          "microphonePermission": "Permite acceso al micr칩fono para funciones interactivas.",
          "recordAudioAndroid": true
        }
      ],
      "expo-roomplan",
      ["expo-build-properties", { "ios": { "deploymentTarget": "17.0" } }]
    ],
    "experiments": {
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "b0a42bfc-cb0b-45a3-8181-e9399bb7ca80"
      }
    }
